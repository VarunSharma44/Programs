****    #include<stdio.h>
    void main(){
     int arr[2][2][2]={{1,2},{3,4},{153,5},{6,8}};
     for(int i=0;i<2;i++){
      for(int j=0;j<2;j++){
       for(int k=0;k<2;k++){
        if (arm(arr[i][j][k]))
        {
         arr[i][j][k]=121;
        }
       }
      }
     }
    }
    
    int arm(int n){
     int c,c=n;
     int arm=0,rem;
     while(n>0){
      rem=n%10;
      arm=arm+(rem*rem*rem);
      n=n/10;
     }
     if(n==1) 
     return 1;
     else
     return 0;
     }
    
